{
{{#if react}}
  "presets": ["@babel/preset-react"],
{{/if}}
  "plugins": [
    "@babel/plugin-proposal-function-bind",
    "@babel/plugin-transform-runtime"
  ],
  "env" : {
    "development": {
{{#if react}}
      "plugins": ["react-hot-loader/babel"],
{{/if}}
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": [
              "last 1 chrome version"
            ]
          }
        ]
      ]
    },
    "production": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "chrome": 58,
              "ie": 9
            }
          }
        ]
      ]
    }
  }
}
